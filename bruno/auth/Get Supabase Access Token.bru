meta {
  name: Get Supabase Access Token
  type: http
  seq: 1
}

post {
  url: {{supabaseUrl}}/auth/v1/token?grant_type=password
  body: json
  auth: inherit
}

params:query {
  grant_type: password
}

headers {
  apikey: {{supabaseAnonKey}}
}

body:json {
  {
    "email": "{{email}}",
    "password": "{{password}}"
  }
}

settings {
  encodeUrl: true
}
