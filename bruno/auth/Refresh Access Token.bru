meta {
  name: Refresh Access Token
  type: http
  seq: 2
}

post {
  url: {{supabaseUrl}}/auth/v1/token?grant_type=refresh_token
  body: json
  auth: inherit
}

params:query {
  grant_type: refresh_token
}

headers {
  apikey: {{supabaseAnonKey}}
}

body:json {
  {
    "refresh_token": "{{refreshToken}}"
  }
}

settings {
  encodeUrl: true
}
